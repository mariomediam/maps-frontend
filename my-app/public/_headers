# Configure MIME types for Vite/ES modules
/*.js
  Content-Type: text/javascript

/*.mjs
  Content-Type: text/javascript

/*.ts
  Content-Type: text/javascript

/*.jsx
  Content-Type: text/javascript

/*.tsx
  Content-Type: text/javascript

# Cache static assets
/assets/*
  Cache-Control: public, max-age=31536000, immutable

# Don't cache the main HTML file
/*.html
  Cache-Control: public, max-age=0, must-revalidate
